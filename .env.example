# Server Configuration
PORT=5001
NODE_ENV=development

# MongoDB Connection
# Untuk local: mongodb://localhost:27017/mahasiswa_db
# Untuk production (MongoDB Atlas): mongodb+srv://username:password@cluster.mongodb.net/mahasiswa_db
MONGO_URI=mongodb://localhost:27017/mahasiswa_db

# JWT Secret Key (ganti dengan string random yang panjang untuk production)
JWT_SECRET=your_jwt_secret_key_here

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here
